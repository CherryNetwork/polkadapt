{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "cache": {
      "enabled": true,
      "path": ".cache",
      "environment": "all"
    }
  },
  "projects": {
    "core": {
      "projectType": "library",
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core/src/test.ts",
            "tsConfig": "projects/core/tsconfig.spec.json",
            "karmaConfig": "projects/core/karma.conf.js"
          }
        }
      }
    },
    "substrate-rpc": {
      "projectType": "library",
      "root": "projects/substrate-rpc",
      "sourceRoot": "projects/substrate-rpc/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/substrate-rpc/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/substrate-rpc/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/substrate-rpc/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/substrate-rpc/src/test.ts",
            "tsConfig": "projects/substrate-rpc/tsconfig.spec.json",
            "karmaConfig": "projects/substrate-rpc/karma.conf.js"
          }
        }
      }
    },
    "polkascan": {
      "projectType": "library",
      "root": "projects/polkascan",
      "sourceRoot": "projects/polkascan/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/polkascan/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/polkascan/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/polkascan/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/polkascan/src/test.ts",
            "tsConfig": "projects/polkascan/tsconfig.spec.json",
            "karmaConfig": "projects/polkascan/karma.conf.js"
          }
        }
      }
    },
    "coingecko": {
      "projectType": "library",
      "root": "projects/coingecko",
      "sourceRoot": "projects/coingecko/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/coingecko/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/coingecko/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/coingecko/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/coingecko/src/test.ts",
            "tsConfig": "projects/coingecko/tsconfig.spec.json",
            "karmaConfig": "projects/coingecko/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "core"
}
